Players = game:GetService("Players")
LocalPlayer = Players.LocalPlayer
jumpPowerValue = 80
isJumpPowerEnabled = false
jumpPowerKey = "F"

function setJumpPower()
    if isJumpPowerEnabled and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
        LocalPlayer.Character.Humanoid.JumpPower = jumpPowerValue
    end
end

MainSection:AddToggle({
    Name = "Enable Jump Power",
    Default = false,
    Flag = "2",
    ToolTip = "Increases your jump power.",
    Save = true,
    Callback = function(Value)
        isJumpPowerEnabled = Value
        if not Value then
            LocalPlayer.Character.Humanoid.JumpPower = 50
        else
            setJumpPower()
        end
    end    
})


function toggleJumpPower()
    if isJumpPowerEnabled then
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
            LocalPlayer.Character.Humanoid.JumpPower = (LocalPlayer.Character.Humanoid.JumpPower == 50) and jumpPowerValue or 50
        end
    end
end

UserInputService = game:GetService("UserInputService")

UserInputService.InputBegan:Connect(function(input)
    if isJumpPowerEnabled and input.KeyCode == Enum.KeyCode[jumpPowerKey] then
        toggleJumpPower()
    end
end)


walkToggle = false

MainSection:AddToggle({
    Name = "Faster Walk",
    Default = false,
    Flag = "3",
    ToolTip = "Slighty Increases your walkspeed",
    Save = true,
    Callback = function(Value)
        walkToggle = Value

        if walkToggle then
            while walkToggle do
                if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
                    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 29
                end
                wait(0.01)
            end
        else
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
        end
    end
})




