local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")


local selectedPlayer = nil
local moneyAmount = 10

local playerDropdown

playerDropdown = BankSection:AddDropdown({
	Name = "Select Player",
	Default = "",
    Flag = "53",
    Save = true,
	Options = {},
	Callback = function(Value)
		selectedPlayer = Players:FindFirstChild(Value)
	end
})

BankSection:AddSlider({
	Name = "Set Amount",
	Min = 0,
	Max = 100000,
	Default = 10,
    Flag = "45",
    Save = true,
	Color = Color3.fromRGB(255, 0, 255),
	Increment = 1,
	ValueName = "$",
	Callback = function(Value)
		moneyAmount = Value
	end
})

BankSection:AddButton({
	Name = "Send Money",
	Callback = function()
		if selectedPlayer and moneyAmount then
			local args = {
				[1] = "SendMoney",
				[2] = selectedPlayer,
				[3] = moneyAmount
			}
			ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("Phone"):FireServer(unpack(args))
		end
	end
})

local function updateDropdown()
	local options = {}
	for _, player in ipairs(Players:GetPlayers()) do
		table.insert(options, player.Name)
	end
	playerDropdown:Refresh(options, true)
end

updateDropdown()

Players.PlayerAdded:Connect(updateDropdown)
Players.PlayerRemoving:Connect(updateDropdown)











