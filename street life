 player = game.Players.LocalPlayer
 character = player.Character or player.CharacterAdded:Wait()
 HumanoidRootPart = character:WaitForChild("HumanoidRootPart")
 PathfindingService = game:GetService("PathfindingService")
 RunService = game:GetService("RunService")
 TweenService = game:GetService("TweenService")

player.CharacterAdded:Connect(function()
	character = player.Character or player.CharacterAdded:Wait()
	HumanoidRootPart = character:WaitForChild("HumanoidRootPart")
end)

 CONSTANT_SPEED = 25
 local currentTween

 function teleport(x, y, z)
	 destination = Vector3.new(x, y + 2, z)
	 path = PathfindingService:CreatePath({
		AgentRadius = 2,
		AgentHeight = 5,
		AgentCanJump = true,
		AgentCanClimb = true,
		WaypointSpacing = 2,
		Costs = {}
	})

	path:ComputeAsync(HumanoidRootPart.Position, destination)

	if path.Status == Enum.PathStatus.Success then
		for _, waypoint in pairs(path:GetWaypoints()) do
			local target = waypoint.Position + Vector3.new(0, 2.5, 0)
			if waypoint.Action == Enum.PathWaypointAction.Jump then
				HumanoidRootPart.Parent:FindFirstChildOfClass("Humanoid"):ChangeState(Enum.HumanoidStateType.Jumping)
			end
			local distance = (HumanoidRootPart.Position - target).Magnitude
			local duration = distance / CONSTANT_SPEED
			local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Linear)
			local goal = { CFrame = CFrame.new(target) }

			if currentTween then
				currentTween:Cancel()
			end

			currentTween = TweenService:Create(HumanoidRootPart, tweenInfo, goal)
			currentTween:Play()
			currentTween.Completed:Wait()
		end
	end
end 


 VirtualInputManager = game:GetService("VirtualInputManager")
function doPot(keyCode)
    VirtualInputManager:SendKeyEvent(true, keyCode, false, game)
    task.wait(1.5)
    VirtualInputManager:SendKeyEvent(false, keyCode, false, game)
end

 Players = game:GetService("Players")
 player = Players.LocalPlayer
 vehicleFolder = workspace:FindFirstChild("Vehicles")
 function findPlayerCar()
    return vehicleFolder and vehicleFolder:FindFirstChild(player.Name .. "'s Car")
end

 function tptovehicle()
    local car = findPlayerCar()
    if not (car and car:IsA("Model")) then return end

    local seat = car:FindFirstChild("DriveSeat", true)
    if not seat then return end

    local character = player.Character or player.CharacterAdded:Wait()
    local humanoid = character:FindFirstChildOfClass("Humanoid") or character:WaitForChild("Humanoid")

    if seat.Occupant == humanoid then return end

    character:MoveTo(seat.Position)
    seat:Sit(humanoid)
end

 function teleportCar(x, y, z)
    tptovehicle()

    local car = findPlayerCar()
    if not car or not car:IsA("Model") then
        OrionLib:MakeNotification({
            Name = "Error!",
            Content = "You do not have a car spawned. Please spawn a car manually or in the \"Vehicle tab\"",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
        return
    end

    car.PrimaryPart = car.PrimaryPart or car:FindFirstChild("DriveSeat", true)
    if car.PrimaryPart then
         forward = car.PrimaryPart.CFrame.LookVector.Unit * 0
         offsetCF = CFrame.new(x, y + 2, z) - forward
        car:SetPrimaryPartCFrame(offsetCF)
        task.wait(0.5)
        VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.Space, false, game)
        task.wait(0.2)
        VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.Space, false, game)
        wait(0.5)
        teleport(x, y, z)
    end
end






 function moveAround()
    local VirtualInputManager = game:GetService("VirtualInputManager")
    task.wait(0.1)
    VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.A, false, game)
    task.wait(0.1)
    VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.S, false, game)
    task.wait(0.1)
    VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.W, false, game)
    task.wait(0.1)
    VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.D, false, game)
    task.wait(0.1)
    VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.A, false, game)
    task.wait(0.1)
    VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.S, false, game)
    task.wait(0.1)
    VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.W, false, game)
    task.wait(0.1)
    VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.D, false, game)
    end







function findPrompts(root)
    local prompts = {}
    for _, item in ipairs(root:GetDescendants()) do
        if item:IsA("ProximityPrompt") then
            table.insert(prompts, item)
        end
    end
    return prompts
end


