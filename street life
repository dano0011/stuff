local function createTriggerDropdown()
    if platformOverride == "Mobile" then
        AimbotSection:AddDropdown({
            Name = "Trigger Key",
            Default = "MouseButton1",
            Options = {"MouseButton1", "MouseButton2"},
            Callback = function(v)
                if v == "MouseButton1" then
                    getgenv().Aimbot.Settings.TriggerKey = Enum.UserInputType.MouseButton1
                else
                    getgenv().Aimbot.Settings.TriggerKey = Enum.UserInputType.MouseButton2
                end
            end
        })
    else
        local keybindMapping = {
            A = Enum.KeyCode.A, B = Enum.KeyCode.B, C = Enum.KeyCode.C, D = Enum.KeyCode.D,
            E = Enum.KeyCode.E, F = Enum.KeyCode.F, G = Enum.KeyCode.G, H = Enum.KeyCode.H,
            I = Enum.KeyCode.I, J = Enum.KeyCode.J, K = Enum.KeyCode.K, L = Enum.KeyCode.L,
            M = Enum.KeyCode.M, N = Enum.KeyCode.N, O = Enum.KeyCode.O, P = Enum.KeyCode.P,
            Q = Enum.KeyCode.Q, R = Enum.KeyCode.R, S = Enum.KeyCode.S, T = Enum.KeyCode.T,
            U = Enum.KeyCode.U, V = Enum.KeyCode.V, W = Enum.KeyCode.W, X = Enum.KeyCode.X,
            Y = Enum.KeyCode.Y, Z = Enum.KeyCode.Z,
            MouseButton2 = Enum.UserInputType.MouseButton2
        }
        
        local keys = {}
        for k in pairs(keybindMapping) do
            table.insert(keys, k)
        end
        
        AimbotSection:AddDropdown({
            Name = "Trigger Key",
            Default = "MouseButton2",
            Options = keys,
            Callback = function(v)
                getgenv().Aimbot.Settings.TriggerKey = keybindMapping[v]
            end
        })
    end
end

createTriggerDropdown()

