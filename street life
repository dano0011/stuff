local function getPlayerNames()
    local names = {}
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= LocalPlayer then
            table.insert(names, player.Name)
        end
    end
    return names
end

local whitelistDropdown = AimbotSection:AddDropdown({
    Name = "Whitelist Players",
    Default = {},
    Options = getPlayerNames(),
    Multi = true,
    Callback = function(v)
        getgenv().Aimbot.Settings.WhitelistedPlayers = v
    end    
})

local lastPlayerCount = #Players:GetPlayers()
connections.playerListUpdate = RunService.Heartbeat:Connect(function()
    local currentPlayerCount = #Players:GetPlayers()
    if currentPlayerCount ~= lastPlayerCount then
        lastPlayerCount = currentPlayerCount
        local currentSelection = getgenv().Aimbot.Settings.WhitelistedPlayers or {}
        local newOptions = getPlayerNames()
        local validSelection = {}
        if type(currentSelection) == "table" then
            for _, selectedName in pairs(currentSelection) do
                for _, availableName in pairs(newOptions) do
                    if selectedName == availableName then
                        table.insert(validSelection, selectedName)
                        break
                    end
                end
            end
        end
        getgenv().Aimbot.Settings.WhitelistedPlayers = validSelection
        if whitelistDropdown and whitelistDropdown.Refresh then
            whitelistDropdown:Refresh(newOptions, true)
            if #validSelection > 0 then
                whitelistDropdown:Set(validSelection)
            end
        end
    end
end)

