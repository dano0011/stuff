_G._nospread = false
_G._fullAuto = false
_G._shotgunMode = false
_G._norecoil = false

local gunClient = require(game:GetService("ReplicatedStorage").Modules.GunFramework.GunClient)
local gunUpdate; do
	for _, v in next, debug.getupvalues(gunClient.Init) do
		if type(v) == "table" and rawget(v, "Update") then
			gunUpdate = v.Update
			break
		end
	end
end

local oldUpdate
oldUpdate = hookfunction(gunUpdate, LPH_NO_UPVALUES(function(self, ...)
	if _G._nospread then self.Settings.SpreadFactor = 0 end
	if _G._fullAuto then self.Settings.IsAutomatic = true end
	if _G._shotgunMode then self.Settings.BulletCount = 10 self.Settings.IsShotgun = true end
	if _G._norecoil then self.Settings.RecoilSpeed = 0 self.Settings.Recoil = 0 end
	return oldUpdate(self, ...)
end))

