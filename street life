        getgenv().Config.Esp.BoxColor = ColorValue
    end    
})



ESPSection:AddToggleColorPicker({
    Name = "Enable Skeletons",
    Default = false,
    ColorPickerDefault = Color3.fromRGB(255, 255, 255),
    Flag = "2130",
    Save = true,
    Callback = function(ColorValue, ToggleValue)
        getgenv().Config.Esp.Skeleton = ToggleValue
        getgenv().Config.Esp.SkeletonColor = ColorValue
        for _, plr in pairs(game:GetService("Players"):GetPlayers()) do
            if plr ~= game:GetService("Players").LocalPlayer and plr.Character then
                getgenv().CPESP(plr.Character)
                getgenv().ThanosTheCreatorEspNigger(plr.Character)
            end
        end
    end
})



