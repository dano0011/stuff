                Name = "Success",
                Content = "Successfully purchased " .. args[1] .. " for $" .. args[2],
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        elseif selectedGunName and gunPrices[selectedGunName] then
            local args = {selectedGunName, gunPrices[selectedGunName]}
            replicatedStorage.Remotes:WaitForChild("GunBuy"):FireServer(unpack(args))
            OrionLib:MakeNotification({
                Name = "Success",
