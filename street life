TrollingSection:AddToggle({
    Name = "Spectate Selected Player",
    Default = false,
    Flag = "42",
    Save = true,
    Callback = function(isChecked)
        if selectedPlayer then
            if isChecked then
                spectatePlayer(selectedPlayer)
            else
                stopSpectating()
            end
        end
    end
})





