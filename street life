local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local player = Players.LocalPlayer
local backpack = player:WaitForChild("Backpack")
local originalValues = {}

local function getAllToolsWithSettings()
	local tools = {}
	for _, tool in ipairs(backpack:GetChildren()) do
		if tool:IsA("Tool") and tool:FindFirstChild("Settings") and tool.Settings:IsA("ModuleScript") then
			table.insert(tools, tool)
		end
	end
	return tools
end

GunSection:AddToggle({
	Name = "Inf Ammo",
	Default = false,
	Callback = function(enabled)
		for _, tool in ipairs(getAllToolsWithSettings()) do
			if enabled then
				if not originalValues[tool] then originalValues[tool] = {} end
				if originalValues[tool].Ammo_Client == nil then
					originalValues[tool].Ammo_Client = tool:GetAttribute("Ammo_Client")
				end
				tool:SetAttribute("Ammo_Client", 999999)
			else
				if originalValues[tool] and originalValues[tool].Ammo_Client ~= nil then
					tool:SetAttribute("Ammo_Client", originalValues[tool].Ammo_Client)
				end
			end
		end
	end
})

GunSection:AddToggle({
	Name = "Inf Mag",
	Default = false,
	Callback = function(enabled)
		for _, tool in ipairs(getAllToolsWithSettings()) do
			if enabled then
				if not originalValues[tool] then originalValues[tool] = {} end
				if originalValues[tool].Capacity == nil then
					originalValues[tool].Capacity = tool:GetAttribute("Capacity")
				end
				tool:SetAttribute("Capacity", 99999)
			else
				if originalValues[tool] and originalValues[tool].Capacity ~= nil then
					tool:SetAttribute("Capacity", originalValues[tool].Capacity)
				end
			end
		end
	end
})

