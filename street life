 function TableFind(tbl, value)
    for k, v in tbl do
        if v == value then return k end
        if type(v) == "table" then
            local index = TableFind(v, value)
            if index then return k, index end
        end
    end
    return false
end

for i, v in getgc() do
    if type(v) == "function" and islclosure(v) and not isourclosure(v) then
        local info = debug.info(v, "s")
        if info and string.match(info, "([^%.]+)$") == "Anti_Cheat" then
            local consts = debug.getconstants(v)
            if TableFind(consts, "StrafingNoPhysics") then
                debug.setupvalue(v, 5, function(...) return end)
            end
        end
    end
end

