    local executor = identifyexecutor()

    if executor == "Solara" or executor == "Xeno" or executor == "Zorara" or executor == "JJSpolit" then
        OrionLib:MakeNotification({
            Name = "Error!",
            Content = "Your executor is not supported. You must use an executor with 100 UNC such as wave or awp.",
            Image = "rbxassetid://10723374276",
            Time = 10
        })
    end
end



