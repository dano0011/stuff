    Name = "Whitelist Selected Players",
    Default = false,
    Flag = "51",
    Save = true,
    Callback = function(Value)
        for _, playerName in ipairs(selectedPlayerNames) do
            if Value then
                whitelistedPlayers[playerName] = true
            else
                whitelistedPlayers[playerName] = nil
            end
        end
    end    
})

