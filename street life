SpySection:AddButton({
    Name = "Show Distance",
    Callback = function()
        if selectedPlayerName and selectedPlayerName ~= "None" then
            local player = game.Players:FindFirstChild(selectedPlayerName)
            if player then
                local localPlayer = game.Players.LocalPlayer
                local selectedPlayerChar = player.Character
                local localPlayerChar = localPlayer.Character

                if selectedPlayerChar and localPlayerChar then
                    local distance = (localPlayerChar.HumanoidRootPart.Position - selectedPlayerChar.HumanoidRootPart.Position).Magnitude

                    OrionLib:MakeNotification({
                        Name = "Distance",
                        Content = "Distance to " .. selectedPlayerName .. " is: " .. tostring(math.floor(distance)) .. " studs",
                        Image = "rbxassetid://4483345998",
                        Time = 5
                    })
                end
            end
        end
    end    
})


SpySection:AddButton({
    Name = "Show Player Information",
    Callback = function()
        if selectedPlayerName and selectedPlayerName ~= "None" then
            local player = game.Players:FindFirstChild(selectedPlayerName)
            if player then
                local username = player.Name
                local userId = player.UserId
                local accountAge = player.AccountAge
                local character = player.Character
                local displayName = player.DisplayName
                local lastLocation = character and character:FindFirstChild("HumanoidRootPart") and character.HumanoidRootPart.Position or nil

                OrionLib:MakeNotification({
                    Name = "Player Information",
                    Content = "Username: " .. username ..
                            "\nDisplay Name: " .. displayName ..
                            "\nUser ID: " .. userId ..
                            "\nAccount Age: " .. accountAge .. " days" ..
                            "\nLast Known Location: " .. (lastLocation and tostring(lastLocation) or "N/A"),
                    Image = "rbxassetid://4483345998",
                    Time = 5
                })
            end
        end
    end
})



Players.PlayerAdded:Connect(function()
    updateDropdown()
end)

Players.PlayerRemoving:Connect(function()
    updateDropdown()
end)

updateDropdown()




