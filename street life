local function chooseMethod(x, y, z)
	local targetCFrame = CFrame.new(x, y, z)
	if tpMethod == "Tween Teleport" then
       teleport(x,y,z)
	else
		teleportCar(x, y, z)
	end
end


FarmSection:AddDivider("Farming - Utilities")

local mopJob = findPrompts(workspace.Map.Jobs.CleanNPC)

local autofarmEnabled = false

FarmSection:AddToggle({
	Name = "Enable Mop Autofarm",
	Default = false,
    ToolTip = "Make sure to claim the job before using!",
	Callback = function(Value)
		autofarmEnabled = Value

		if autofarmEnabled then
			task.spawn(function()
				while autofarmEnabled do
					for i = 1, 5 do
						if not autofarmEnabled then return end
						local job = workspace.Map.Jobs.CleanNPC.Clean[tostring(i)]
						if job and job:FindFirstChild("CleanDecal") and job.CleanDecal.Transparency == 0.25 then
							local pos = job.CleanDecal.Parent.Position
							chooseMethod(pos.X, pos.Y, pos.Z)
							wait(0.3)
							for _, v in ipairs(job:GetDescendants()) do
								if v:IsA("ProximityPrompt") then
									fireproximityprompt(v)
								end
							end
							wait(4)
						end
					end
					wait(0.1)
				end
			end)
		end
	end
})




local visitedTrash = {}

function teleportToNearestTrash()
	local player = game.Players.LocalPlayer
	if not player.Character or not player.Character:FindFirstChild("HumanoidRootPart") then return end

	local closestTrash = nil
	local shortestDistance = math.huge
	local playerPosition = player.Character.HumanoidRootPart.Position

	for _, obj in pairs(workspace.Map.Interactions:GetChildren()) do
		if obj:IsA("MeshPart") and obj.Name == "Search Trash" and not visitedTrash[obj] then
			local distance = (obj.Position - playerPosition).Magnitude
			if distance < shortestDistance then
				closestTrash = obj
				shortestDistance = distance
			end
		end
	end

	if closestTrash then
		visitedTrash[closestTrash] = true
		chooseMethod(closestTrash.Position.X, closestTrash.Position.Y, closestTrash.Position.Z)
	end
end

local trashEnabled = false

FarmSection:AddToggle({
	Name = "Enable Trash Autofarm",
	Default = false,
	Callback = function(Value)
		trashEnabled = Value
		if Value then
			task.spawn(function()
				while trashEnabled do
					local found = false

					for _, obj in pairs(workspace.Map.Interactions:GetChildren()) do
						if obj:IsA("MeshPart") and obj.Name == "Search Trash" and not visitedTrash[obj] then
							found = true
							break
						end
					end

					if not found then
						visitedTrash = {} 
					end

					teleportToNearestTrash()
					wait(0.3)
					doPot(Enum.KeyCode.E)
					wait(0.3)
				end
			end)
		end
	end
})

local carRobFarmRunning = false

FarmSection:AddToggle({
	Name = "Enable Car rob autofarm",
	Default = false,
	Callback = function(Value)
		carRobFarmRunning = Value

		if Value then
			task.spawn(function()
				while carRobFarmRunning do
					for _, obj in ipairs(workspace:GetDescendants()) do
						if not carRobFarmRunning then break end
						if obj:IsA("Part") and obj.Name == "H" then
							local pos = obj.Position
							chooseMethod(pos.X, pos.Y, pos.Z)
							wait(0.2)

							local stuffT = findPrompts(workspace.Map.Interactions)
							for _, prompt in ipairs(stuffT) do
								fireproximityprompt(prompt)
							end

							wait(2)

							for _, prompt in ipairs(stuffT) do
								fireproximityprompt(prompt)
							end

							wait(0.3)
						end
					end
					wait(0.1)
				end
			end)
		end
	end
})




