    Name = "Loop Bring Player to you",
    Default = false,
    Callback = function(Value)
        if Value then
            loopBringPlayer()
        else
            stopLoopBring()
        end
    end
})









TrollingSection:AddDivider("Spin Bot")

 power = 100

TrollingSection:AddToggle({
	Name = "Enable Spin-Bot",
	Default = false,
	Callback = function(Value)
		if Value then
			game:GetService('RunService').Stepped:connect(function()
				game.Players.LocalPlayer.Character.Head.CanCollide = false
				game.Players.LocalPlayer.Character.UpperTorso.CanCollide = false
				game.Players.LocalPlayer.Character.LowerTorso.CanCollide = false
				game.Players.LocalPlayer.Character.HumanoidRootPart.CanCollide = false
			end)
			wait(0.1)
			bambam = Instance.new("BodyThrust")
			bambam.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
			bambam.Force = Vector3.new(power, 0, power)
			bambam.Location = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
		else
			if bambam then
				bambam:Destroy()
			end
		end
	end    
})

TrollingSection:AddSlider({
	Name = "Power",
	Min = 20,
	Max = 300,
	Default = 100,
    Flag = "gefw2",
    Save = true,
	Color = Color3.fromRGB(255, 0, 255),
	Increment = 1,
	ValueName = "Power",
	Callback = function(Value)
		power = Value
		if bambam then
			bambam.Force = Vector3.new(power, 0, power)
		end
	end    
})



TrollingSection:AddDivider("Other")
 function isR15(character)
    return character:FindFirstChild("UpperTorso") and character:FindFirstChild("LowerTorso")
end


 toggleValue = false
local bangAnim
local bang

TrollingSection:AddToggle({
    Name = "Play Bang Animation",
    Default = false,
    Callback = function(Value)
        toggleValue = Value
        local humanoid = game.Players.LocalPlayer.Character:FindFirstChild("Humanoid")

        if humanoid then
            if toggleValue and not bang then
                bangAnim = Instance.new("Animation")
                bangAnim.AnimationId = isR15(game.Players.LocalPlayer.Character) and "rbxassetid://5918726674" or "rbxassetid://148840371"
                bang = humanoid:LoadAnimation(bangAnim)
                bang:Play(0.1, 1, 1)
                bang:AdjustSpeed(3)

                local bangDied
                bangDied = humanoid.Died:Connect(function()
                    bang:Stop()
                    bangAnim:Destroy()
                    bangDied:Disconnect()
                end)
            elseif not toggleValue and bang then
                bang:Stop()
                bangAnim:Destroy()
                bang = nil
                bangAnim = nil
            end
        end
    end    
})






SpySection:AddDivider("Player Selection")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local selectedPlayerName = "None"
local Dropdown

local function getPlayerNames()
    local t = {"None"}
    for _, p in ipairs(game.Players:GetPlayers()) do
        table.insert(t, p.Name)
    end
    return t
end

Dropdown = SpySection:AddDropdown({
    Name = "Select Player",
    Default = "None",
    Flag = "54",
    Save = true,
    Options = getPlayerNames(),
    Callback = function(v)
        selectedPlayerName = v
    end
})

task.spawn(function()
    while task.wait(1) do
        Dropdown:Refresh(getPlayerNames(), true)
    end
end)


local function getPlayerData(player, dataName)
    if player and player:FindFirstChild("Data") then
        local data = player.Data:FindFirstChild(dataName)
        return data and data.Value or "N/A"
    end
    return "N/A"
end

SpySection:AddDivider("Spies")

SpySection:AddButton({
    Name = "Show Backpack",
    Callback = function()
        local selectedPlayer = Players:FindFirstChild(selectedPlayerName)
        
        if selectedPlayer and selectedPlayerName ~= "None" then
            local inventoryItems = selectedPlayer:FindFirstChild("Backpack") and selectedPlayer.Backpack:GetChildren() or {}
            
            local itemList = {}
            for _, item in ipairs(inventoryItems) do
                if item.Name ~= "Tools" and item.Name ~= "LocalFootsteps" then
                    table.insert(itemList, item.Name)
                end
            end
            
            local itemsString = table.concat(itemList, ", ") or "No items in backpack."
            
            OrionLib:MakeNotification({
                Name = selectedPlayer.Name,
                Content = itemsString,
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        else
            OrionLib:MakeNotification({
                Name = "Error",
                Content = "No player selected or player not found.",
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        end
    end    
})


SpySection:AddButton({
    Name = "Show Money",
    Callback = function()
        local selectedPlayer = Players:FindFirstChild(selectedPlayerName)
        if selectedPlayer and selectedPlayerName ~= "None" then
            local money = getPlayerData(selectedPlayer, "Money")
            OrionLib:MakeNotification({
                Name = selectedPlayer.Name,
                Content = "Money: " .. money,
                Image = "rbxassetid://4483345998",
