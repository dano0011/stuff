local UserInputService = game:GetService("UserInputService")

 local currentPlatform = UserInputService.TouchEnabled and "Mobile" or "PC"
local platformOverride = currentPlatform


getgenv().Aimbot = {
    Settings = {
        Enabled = false,
        TeamCheck = false,
        AliveCheck = false,
        WallCheck = false,
        Smoothness = 0,
        LockPart = "Head",
        TriggerKey = (platformOverride == "Mobile") and Enum.UserInputType.MouseButton1 or Enum.UserInputType.MouseButton2,
        Toggle = false,
        WhitelistEnabled = false,
        WhitelistedPlayers = {},
        FOVMode = "On Cursor",
        Prediction = 0,
        Sensitivity = 1
    },
    FOVSettings = {
        Enabled = true,
        Visible = false,
        Radius = 200,
        NumSides = 60,
        Thickness = 2,
        Transparency = 1,
        Filled = false,
        Color = Color3.fromRGB(255, 255, 255),
        LockedColor = Color3.fromRGB(255, 0, 255)
    }
}

local aimbotRunning = false
local currentTarget = nil
local fovCircle = Drawing.new("Circle")
local connections = {}

fovCircle.Visible = false
fovCircle.Filled = false

function updateTriggerKey()
    getgenv().Aimbot.Settings.TriggerKey = (platformOverride == "Mobile") and Enum.UserInputType.MouseButton1 or Enum.UserInputType.MouseButton2
    
    if platformOverride == "Mobile" then
        getgenv().Aimbot.Settings.FOVMode = "Middle"
    else
        getgenv().Aimbot.Settings.FOVMode = "On Cursor"
    end
end
local Camera = workspace.CurrentCamera

function getClosestPlayer()
    local settings = getgenv().Aimbot.Settings
    local fovSettings = getgenv().Aimbot.FOVSettings
    local closestPlayer = nil
    local shortestDistance = math.huge
    local mousePos = UserInputService:GetMouseLocation()
    if settings.FOVMode == "Middle" then
        local viewportSize = Camera.ViewportSize
        mousePos = Vector2.new(viewportSize.X / 2, viewportSize.Y / 2)
    end
    
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild(settings.LockPart) then
            local character = player.Character
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            
            if humanoid and humanoid.Health > 0 then
                local skipPlayer = false
                
                if settings.TeamCheck and player.Team == LocalPlayer.Team then
                    skipPlayer = true
                end
                
                if settings.WhitelistEnabled then
                    local isWhitelisted = false
                    if type(settings.WhitelistedPlayers) == "table" then
                        for _, whitelistedName in pairs(settings.WhitelistedPlayers) do
                            if player.Name == whitelistedName then
                                isWhitelisted = true
                                break
                            end
                        end
                    end
                    if isWhitelisted then
                        skipPlayer = true
                    end
                end
                
                if not skipPlayer then
                    local targetPart = character[settings.LockPart]
                    local worldPos, onScreen = Camera:WorldToViewportPoint(targetPart.Position)
                    
                    if onScreen then
                        if settings.WallCheck then
                            local ray = workspace:Raycast(Camera.CFrame.Position, (targetPart.Position - Camera.CFrame.Position).Unit * 1000, {
                                ignorelist = {LocalPlayer.Character}
                            })
                            if ray and ray.Instance and not ray.Instance:IsDescendantOf(character) then
                                skipPlayer = true
                            end
                        end
                        
                        if not skipPlayer then
                            local screenPos = Vector2.new(worldPos.X, worldPos.Y)
                            local distance = (mousePos - screenPos).Magnitude
                            
                            if platformOverride == "Mobile" then
                                local characterDistance = (LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")) and
                                    (LocalPlayer.Character.HumanoidRootPart.Position - character.HumanoidRootPart.Position).Magnitude or math.huge
                                if characterDistance < shortestDistance then
                                    shortestDistance = characterDistance
                                    closestPlayer = player
                                end
                            else
                                if distance < shortestDistance then
                                    if fovSettings.Enabled then
                                        if distance <= fovSettings.Radius then
                                            shortestDistance = distance
                                            closestPlayer = player
                                        end
                                    else
                                        if (mousePos - screenPos).Magnitude <= 200 then
                                            shortestDistance = distance
                                            closestPlayer = player
                                        end
                                    end
                                end
                            end
                        end
                    end
                end
            end
        end
    end
    
    return closestPlayer
end

function aimAtTarget(target)
    if not target or not target.Character then return end
    
    local settings = getgenv().Aimbot.Settings
    local targetPart = target.Character:FindFirstChild(settings.LockPart)
    if not targetPart then return end
    
    local targetPosition = targetPart.Position
    local velocity = targetPart.Velocity
    local prediction = settings.Prediction
    
    if prediction > 0 and velocity then
        local bulletTime = prediction * 0.016
        targetPosition = targetPosition + (velocity * bulletTime)
    end
    
    local sensitivity = settings.Sensitivity or 1
    if platformOverride == "Mobile" then
        if settings.Smoothness > 0 then
            local currentCFrame = Camera.CFrame
            local targetCFrame = CFrame.new(currentCFrame.Position, targetPosition)
            Camera.CFrame = currentCFrame:Lerp(targetCFrame, math.clamp(sensitivity * (1 / (settings.Smoothness * 10 + 1)), 0, 1))
        else
            Camera.CFrame = CFrame.new(Camera.CFrame.Position, targetPosition)
        end
    else
        if settings.Smoothness > 0 then
            local currentCFrame = Camera.CFrame
            local targetCFrame = CFrame.new(currentCFrame.Position, targetPosition)
            local smoothness = settings.Smoothness / 10
            local alpha = math.clamp((1 - smoothness) * sensitivity, 0, 1)
            Camera.CFrame = currentCFrame:Lerp(targetCFrame, alpha)
        else
            Camera.CFrame = CFrame.new(Camera.CFrame.Position, targetPosition)
        end
    end
end

function updateFOVCircle()
    local settings = getgenv().Aimbot.Settings
    local fovSettings = getgenv().Aimbot.FOVSettings
    
    if fovSettings.Enabled and fovSettings.Visible and settings.Enabled then
        fovCircle.Visible = true
        fovCircle.Radius = fovSettings.Radius
        fovCircle.NumSides = fovSettings.NumSides
        fovCircle.Thickness = fovSettings.Thickness
        fovCircle.Transparency = fovSettings.Transparency
        fovCircle.Filled = fovSettings.Filled
        fovCircle.Color = currentTarget and fovSettings.LockedColor or fovSettings.Color
        if settings.FOVMode == "Middle" then
            local viewportSize = Camera.ViewportSize
            fovCircle.Position = Vector2.new(viewportSize.X / 2, viewportSize.Y / 2)
        else
            fovCircle.Position = UserInputService:GetMouseLocation()
        end
    else
        fovCircle.Visible = false
        fovCircle.FilledColor = Color3.new(0,0,0)
    end
end

local lastTargetTime = 0
local targetLostTime = 0

connections.renderStepped = RunService.RenderStepped:Connect(function()
    local settings = getgenv().Aimbot.Settings
    updateFOVCircle()
    
    if settings.Enabled then
        local currentTime = tick()
        local isTargetValid = currentTarget and 
            currentTarget.Character and 
            currentTarget.Character:FindFirstChild(settings.LockPart) and
            (currentTarget.Character:FindFirstChildOfClass("Humanoid") and 
             currentTarget.Character:FindFirstChildOfClass("Humanoid").Health > 0)
        
        if currentTarget and not isTargetValid then
            targetLostTime = currentTime
            currentTarget = nil
        end
        
        local shouldUpdateTarget = not currentTarget
        
        if platformOverride == "Mobile" then
            if aimbotRunning then
                local newTarget = getClosestPlayer()
                if newTarget then
                    currentTarget = newTarget
                    lastTargetTime = currentTime
                    if isTargetValid then
                        aimAtTarget(currentTarget)
                    end
                end
            else
                currentTarget = nil
            end
        elseif aimbotRunning then
            if shouldUpdateTarget then
                local newTarget = getClosestPlayer()
                if newTarget then
                    currentTarget = newTarget
                    lastTargetTime = currentTime
                end
            end
            if currentTarget and isTargetValid then
                aimAtTarget(currentTarget)
            end
        end
    else
        currentTarget = nil
        targetLostTime = tick()
    end
end)

local lastTarget = nil

connections.inputBegan = UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    
    local settings = getgenv().Aimbot.Settings
    
    if platformOverride == "Mobile" then
        if input.UserInputType == settings.TriggerKey or input.UserInputType == Enum.UserInputType.Touch then
            if settings.Toggle then
                aimbotRunning = not aimbotRunning
                if not aimbotRunning then
                    currentTarget = nil
                end
            else
                aimbotRunning = true
            end
        end
    else
        if input.UserInputType == settings.TriggerKey or 
           (input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == settings.TriggerKey) then
            if settings.Toggle then
                aimbotRunning = not aimbotRunning
                if not aimbotRunning then
                    currentTarget = nil
                end
            else
                aimbotRunning = true
            end
        end
    end
end)

connections.inputEnded = UserInputService.InputEnded:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    
    local settings = getgenv().Aimbot.Settings
    
    if not settings.Toggle then
        if platformOverride == "Mobile" then
            if input.UserInputType == settings.TriggerKey or input.UserInputType == Enum.UserInputType.Touch then
                aimbotRunning = false
                lastTarget = currentTarget
                currentTarget = nil
            end
        else
            if input.UserInputType == settings.TriggerKey or 
               (input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == settings.TriggerKey) then
                aimbotRunning = false
                lastTarget = currentTarget
                currentTarget = nil
            end
        end
    end
end)







