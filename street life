local dropdownOptions = {}
for name in pairs(soundMap) do
    table.insert(dropdownOptions, name)
end
table.sort(dropdownOptions)

OtherSection:AddDropdown({
    Name = "Select Track",
    Default = dropdownOptions[1],
    Options = dropdownOptions,
    Callback = function(Value)
        soundID = soundMap[Value]
    end
})

OtherSection:AddTextbox({
    Name = "Custom Sound ID",
    Default = "",
    TextDisappear = false,
    Callback = function(Value)
        soundID = Value
    end
})

OtherSection:AddToggle({
    Name = "Play Selected Track",
    Default = false,
    Callback = function(Value)
        if Value and soundID then
            if music then
                music:Stop()
                music:Destroy()
            end
            music = Instance.new("Sound")
            music.SoundId = "rbxassetid://" .. soundID
            music.Volume = 5
            music.Looped = true
            music.Parent = game.Workspace
            music:Play()
        elseif music then
            music:Stop()
            music:Destroy()
            music = nil
        end
    end
})

OtherSection:AddSlider({
    Name = "Volume",
    Min = 0,
    Max = 10,
    Default = 5,
    Color = Color3.fromRGB(255, 0, 255),
    Increment = 1,
    Flag = "61",
    Save = true,
    ValueName = "Volume",
    Callback = function(Value)
        if music then
            music.Volume = Value
        end
    end
})

ShowTimePlayed = false
ShowDOGE = false
ShowCrypto = false
ShowBank = false
ShowLevel = false
ShowETH = false

playerJoinTime = tick()

function WebhookManager()
    spawn(function()
        while WebhookLog do
            task.wait(WebhookDelay)
            OSTime = os.time()
            playerLocalTime = os.date('*t', OSTime)
            formattedLocalTime = string.format('%02d:%02d:%02d %s',
                (playerLocalTime.hour % 12 == 0 and 12 or playerLocalTime.hour % 12),
                playerLocalTime.min,
                playerLocalTime.sec,
                playerLocalTime.hour < 12 and "AM" or "PM"
            )
            
            player = game.Players.LocalPlayer
            playerUsername = player.Name

            timePlayed = (tick() - playerJoinTime) / 60
            DOGE = player.Data and player.Data.DOGE and player.Data.DOGE.Value or 0
            Crypto = player.Data and player.Data.Crypto and player.Data.Crypto.Value or 0
            Bank = player.Data and player.Data.Bank and player.Data.Bank.Value or 0
            Level = player.Data and player.Data.Level and player.Data.Level.Value or 0
            ETH = player.Data and player.Data.ETH and player.Data.ETH.Value or 0

            Embed = {
                title = 'Street Warz 2 WebHook Manager',
                description = '**Player Stats:**\nUsername: ' .. playerUsername,
                color = 0x00FF00,
                fields = {},
                timestamp = os.date('!%Y-%m-%dT%H:%M:%SZ', OSTime),
            }

            if ShowTimePlayed then
                table.insert(Embed.fields, {
                    name = 'Time Played â±',
                    value = string.format('\n%.2f minutes\n', timePlayed),
                    inline = true
                })
            end

            if ShowDOGE then
                table.insert(Embed.fields, {
                    name = 'DOGE ðŸª™',
                    value = '\n' .. tostring(DOGE) .. '\n',
                    inline = true
                })
            end

            if ShowCrypto then
                table.insert(Embed.fields, {
                    name = 'Crypto ðŸª™',
                    value = '\n' .. tostring(Crypto) .. '\n',
                    inline = true
                })
            end

            if ShowBank then
                table.insert(Embed.fields, {
                    name = 'Bank ðŸ’¼',
                    value = '\n' .. tostring(Bank) .. '\n',
                    inline = true
                })
            end

            if ShowLevel then
                table.insert(Embed.fields, {
                    name = 'Level âš¡',
                    value = '\n' .. tostring(Level) .. '\n',
                    inline = true
                })
            end

            if ShowETH then
                table.insert(Embed.fields, {
                    name = 'ETH ðŸª™',
                    value = '\n' .. tostring(ETH) .. '\n',
                    inline = true
                })
            end

            success, response = pcall(function()
                return (syn and syn.request or http_request) {
                    Url = WebhookUrl,
                    Method = 'POST',
                    Headers = { ['Content-Type'] = 'application/json' },
                    Body = game:GetService('HttpService'):JSONEncode({
                        username = 'Express Hub Webhook',
                        avatar_url = 'https://cdn.discordapp.com/attachments/1267926453786382406/1317651435990683648/IMG_3318.jpg?ex=675f7607&is=675e2487&hm=48744c2263e268f2154c244bb2a3af01ee1d54165c2de83ed3d5a27166b31ebe&',
                        embeds = { Embed }
                    }),
                }
            end)

            if not success then
                warn("Failed to send data to webhook:", response)
            else
                print("Webhook response:", response.StatusCode, response.Body)
            end
        end
    end)
end


