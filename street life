PhoneSection:AddDivider("Ratting")


PhoneSection:AddButton({
	Name = "Send tweet with brokest player in server",
	Callback = function()
		local RepStorage = game:GetService("ReplicatedStorage")
		local PhoneRemote = RepStorage:WaitForChild("Remotes"):WaitForChild("Phone")
		local Players = game:GetService("Players")

		local richestPlayer = nil
		local poorestPlayer = nil
		local maxMoney = -math.huge
		local minMoney = math.huge

		for _, player in ipairs(Players:GetPlayers()) do
			local moneyObj = player:FindFirstChild("Data") and player.Data:FindFirstChild("Money")
			if moneyObj then
				local moneyVal = moneyObj.Value
				if moneyVal > maxMoney then
					maxMoney = moneyVal
					richestPlayer = player
				end
				if moneyVal < minMoney then
					minMoney = moneyVal
					poorestPlayer = player
				end
			end
		end

		if poorestPlayer then
			local args = {
				"SendMessage",
				poorestPlayer.Name.." has $"..minMoney.." and is the brokest in the server"
			}
			PhoneRemote:FireServer(unpack(args))
		end

		if richestPlayer then
			local args = {
				"SendMessage",
				richestPlayer.Name.." has $"..maxMoney.." and is the best in the server"
			}
			PhoneRemote:FireServer(unpack(args))
		end
	end    
})


ab = CombatTab:AddSubTab({Name = "Aimlock"})
 AimbotSection = ab:AddSection({
