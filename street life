ESPSection:AddToggleColorPicker({
    Name = "Enable Boxes",
    Default = false,
    ColorPickerDefault = Color3.fromRGB(255, 255, 255),
    Flag = "highlight_toggle",
    Save = true,
    Callback = function(ColorValue, ToggleValue)
