Players.PlayerRemoving:Connect(updateDropdown)







GUISection:AddDivider("Gui's")


 folder = workspace.Map.FlipsyPrompts
 nameCount = {}
for _, model in pairs(folder:GetChildren()) do
    if model:IsA("Model") then
         upperTorso = model:FindFirstChild("UpperTorso", true)
        if upperTorso then
             handler = upperTorso:FindFirstChild("Handler")
            if handler and handler:IsA("ProximityPrompt") then
                 baseName = model.Name
                nameCount[baseName] = (nameCount[baseName] or 0) + 1
                 displayName = nameCount[baseName] > 1 and baseName .. " " .. nameCount[baseName] or "Open " .. baseName
                GUISection:AddButton({
                    Name = displayName,
                    Callback = function()
                        fireproximityprompt(handler)
                    end
                })
            end
        end
    end
end

StoreSection:AddDivider("Items")


local replicatedStorage = game:GetService("ReplicatedStorage")
local selectedCategory = nil
local selectedItem = nil
local itemPrices = {}

function createDropdown(name, folderPath, categoryName)
    local items = {}
    local nameMap = {}

    for _, clothingItem in pairs(folderPath:GetChildren()) do
        local itemName, itemPrice

        for _, child in pairs(clothingItem:GetChildren()) do
            if child:IsA("StringValue") then
                itemName = child.Value
            elseif child:IsA("NumberValue") then
                itemPrice = child.Value
            end
        end

        if itemName and itemPrice then
            local displayName = itemName .. " ($" .. itemPrice .. ")"
            table.insert(items, displayName)
            nameMap[displayName] = clothingItem.Name
            itemPrices[clothingItem.Name] = itemPrice
        end
    end

    StoreSection:AddDropdown({
        Name = name,
        Default = "",
        Options = items,
        Callback = function(Value)
            selectedCategory = categoryName
            selectedItem = nameMap[Value]
        end    
    })
end

createDropdown("Pants", replicatedStorage.Misc.Clothing.PANTS, "PANTS")
createDropdown("Shirts", replicatedStorage.Misc.Clothing.SHIRTS, "SHIRTS")





