local locked = false
local connection

FarmSection:AddToggle({
	Name = "Lock Camera Postion",
	Default = false,
    ToolTip = "Locks your camera in a postion better for farming",
	Callback = function(Value)
		local player = game:GetService("Players").LocalPlayer
		local character = player.Character or player.CharacterAdded:Wait()
		local rootPart = character:WaitForChild("HumanoidRootPart")
		local camera = workspace.CurrentCamera

		if Value then
			camera.CameraType = Enum.CameraType.Scriptable
			connection = game:GetService("RunService").RenderStepped:Connect(function()
				camera.CFrame = CFrame.new(rootPart.Position + Vector3.new(1, 3, 3), rootPart.Position)
			end)
		else
			if connection then
				connection:Disconnect()
				connection = nil
			end
			camera.CameraType = Enum.CameraType.Custom
		end
	end
})



CryptoSection:AddDivider("Crypto - Instructions")

CryptoSection:AddParagraph("Auto Trading Instructions", "Use the sliders to set the buy and sell price limits. Turn on auto trading to automatically buy when the price falls below the buy limit and sell when it rises above the sell limit.")


CryptoSection:AddDivider(" Crypto Auto trade")

local selectedCrypto = "Bitcoin"
local buyThreshold = 0
local sellThreshold = 0
local isAutoTradingEnabled = false

CryptoSection:AddDropdown({
    Name = "Select Cryptocurrency",
    Default = "",
    Flag = "66",
    Save = true,
    Options = {"Bitcoin", "Dogecoin", "Ethereum"},
    Callback = function(Value)
        selectedCrypto = Value
    end    
})

CryptoSection:AddSlider({
    Name = "Buy Threshold",
    Min = 0,
    Max = 10000,
    Default = 0,
    Flag = "40",
    Save = true,
    Color = Color3.fromRGB(255, 0, 255),
    Increment = 1,
    ValueName = "$",
    Callback = function(Value)
        buyThreshold = Value
    end    
})

CryptoSection:AddSlider({
    Name = "Sell Threshold",
    Min = 0,
    Max = 10000,
    Default = 0,
    Flag = "833",
    Save = true,
    Color = Color3.fromRGB(255, 0, 255),
    Increment = 1,
    ValueName = "$",
    Callback = function(Value)
        sellThreshold = Value
    end    
})

CryptoSection:AddToggle({
    Name = "Enable Auto Trading",
    Default = false,
    Callback = function(Value)
        isAutoTradingEnabled = Value
    end    
})

spawn(function()
    while true do
        if isAutoTradingEnabled then
            local cryptoType = ""
            local currentLabel = ""
            
            if selectedCrypto == "Bitcoin" then
                cryptoType = "Crypto"
                currentLabel = "CryptoCurrent"
            elseif selectedCrypto == "Dogecoin" then
                cryptoType = "DOGE"
                currentLabel = "DOGECurrent"
            elseif selectedCrypto == "Ethereum" then
                cryptoType = "ETH"
                currentLabel = "ETHCurrent"
            end
            
            local cryptoPriceText = game:GetService("Players").LocalPlayer.PlayerGui.PhoneUI.Main.CurrencyFrame[currentLabel].Text
            local cleanPriceText = cryptoPriceText:gsub("[^%d.]", "")
            local cryptoPrice = tonumber(cleanPriceText)
            
            if cryptoPrice then
                if cryptoPrice <= buyThreshold then
                    local args = {
                        [1] = cryptoType,
                        [2] = "Purchase",
                        [3] = cryptoPrice
                    }
                    game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Phone"):FireServer(unpack(args))
                elseif cryptoPrice >= sellThreshold then
                    local args = {
                        [1] = cryptoType,
                        [2] = "Sell",
                        [3] = cryptoPrice
                    }
                    game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Phone"):FireServer(unpack(args))
                end
            end
        end
        task.wait(1)
    end
end)



