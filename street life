ChangerSection:AddDivider("Rainbow")

local rainbowUsernameEnabled = false
local rainbowLevelEnabled = false

local originalUsernameColor = workspace[playerName].Head.Overhead.Username.TextColor3
local originalLevelColor = workspace[playerName].Head.Overhead.LevelIcon.Level.TextColor3

task.spawn(function()
    while true do
        if rainbowUsernameEnabled and userToggleEnabled then
            local hue = tick() % 5 / 5
            local color = Color3.fromHSV(hue, 1, 1)
            workspace[playerName].Head.Overhead.Username.TextColor3 = color
        end
        if rainbowLevelEnabled and levelToggleEnabled then
            local hue = tick() % 5 / 5
            local color = Color3.fromHSV(hue, 1, 1)
            workspace[playerName].Head.Overhead.LevelIcon.Level.TextColor3 = color
        end
        task.wait()
    end
end)

ChangerSection:AddToggle({
    Name = "Rainbow Username",
    Default = false,
    Flag = "13",
    Save = true,
    Callback = function(Value)
        rainbowUsernameEnabled = Value
        if not Value and userToggleEnabled then
            workspace[playerName].Head.Overhead.Username.TextColor3 = originalUsernameColor
        end
    end
})

ChangerSection:AddToggle({
    Name = "Rainbow Level",
    Default = false,
    Flag = "14",
    Save = true,
    Callback = function(Value)
        rainbowLevelEnabled = Value
        if not Value and levelToggleEnabled then
            workspace[playerName].Head.Overhead.LevelIcon.Level.TextColor3 = originalLevelColor
        end
    end
})








