                Image = "rbxassetid://4483345998",
                Time = 5
            })
        end
    end    
})








SafeSection:AddDivider("Items")

local players = game:GetService("Players")
local player = players.LocalPlayer
local backpack = player:WaitForChild("Backpack")
local selectedSafe

local function getItemsFromBackpack()
    local items = {}
    for _, item in pairs(backpack:GetChildren()) do
        if item:IsA("Tool") then
            table.insert(items, item.Name)
        end
    end
    return items
end

local function getItemsFromSafeData()
    local items = {}
    local safeItems = player:FindFirstChild("Data") and player.Data:FindFirstChild("SafeItems")
    if safeItems then
        for _, item in pairs(safeItems:GetChildren()) do
            table.insert(items, item.Name)
        end
    end
    return items
end

local DropdownBackpack = SafeSection:AddDropdown({
    Name = "Backpack Items",
    Default = "1",
    Flag = "64",
    Save = true,
    Options = getItemsFromBackpack(),
    Callback = function(Value)
        selectedSafe = Value
    end
})

