MiscSection:AddDivider("Character Visuals")

local enableChams = false
local originalColors = {}
local chamsColor = Color3.fromRGB(255, 0, 0)

MiscSection:AddToggleColorPicker({
    Name = "Local Chams",
    Default = false,
    ColorPickerDefault = Color3.fromRGB(255, 0, 0),
    Callback = function(color, toggle)
        enableChams = toggle
        chamsColor = color
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        for _, part in pairs(character:GetChildren()) do
            if part:IsA("BasePart") then
                if enableChams then
                    if not originalColors[part] then
                        originalColors[part] = part.Color
                    end
                    part.Color = chamsColor
                else
                    if originalColors[part] then
                        part.Color = originalColors[part]
                    end
                end
            end
        end
    end
})


game:GetService("RunService").RenderStepped:Connect(function()
    if enableChams then
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        for _, part in pairs(character:GetChildren()) do
            if part:IsA("BasePart") then
                part.Color = chamsColor
            end
        end
    end
end)

local originalMaterials = {}
local originalReflectance = {}
local bodyParts = {
    "LeftHand", "Head", "LeftUpperLeg", "LeftFoot", "LeftUpperArm", "RightLowerArm", "RightUpperArm", "LeftLowerLeg", "RightUpperLeg", "RightHand", "RightFoot", "RightLowerLeg", "LowerTorso", "UpperTorso", "LeftLowerArm", "HumanoidRootPart"
}

MiscSection:AddDropdown({
    Name = "Character Material",
    Default = "",
    Options = {"Default", "ForceField", "Neon"},
    Callback = function(material)
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()

        for _, partName in pairs(bodyParts) do
            local part = character:FindFirstChild(partName)
            if part and part:IsA("BasePart") then
                if material == "Default" then
                    if originalMaterials[part] then
                        part.Material = originalMaterials[part]
                        part.Reflectance = originalReflectance[part] or 0
                    end
                else
                    if not originalMaterials[part] then
                        originalMaterials[part] = part.Material
                        originalReflectance[part] = part.Reflectance
                    end
                    part.Material = Enum.Material[material]
                    if material == "Neon" then
                        part.Reflectance = 0
                        part.Color = Color3.fromRGB(255, 255, 255)
                    else
                        part.Reflectance = originalReflectance[part] or 0
                    end
                end
            end
        end

        local shirt = character:FindFirstChildOfClass("Shirt")
        local pants = character:FindFirstChildOfClass("Pants")

        if shirt then
            if material == "Default" then
                shirt.ShirtTemplate = originalMaterials[shirt] or shirt.ShirtTemplate
            else
                if not originalMaterials[shirt] then
                    originalMaterials[shirt] = shirt.ShirtTemplate
                end
                shirt.ShirtTemplate = ""
            end
        end

        if pants then
            if material == "Default" then
                pants.PantsTemplate = originalMaterials[pants] or pants.PantsTemplate
            else
                if not originalMaterials[pants] then
                    originalMaterials[pants] = pants.PantsTemplate
                end
                pants.PantsTemplate = ""
            end
        end
    end    
})





